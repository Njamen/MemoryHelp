2022/04/27
 
 
         SELECT t1.idStudent, t1.studentNames, t1.studentFirstNames, t1.bithDate, t1.matricule
               t1.birthPlace, t1.idStudentClassAYear,t1.sexe, t1.redouble, t1.present,                 
               t2.markCategory, t2.marksCategoryName, t2.idAbsenceHoursInfo, 
               t2.abscenceHours, t2.justifiedAbscenceHours  

        FROM
        (
            SELECT st.idStudent, st.studentNames, st.studentFirstNames, st.bithDate, 
                    st.birthPlace, 
                                st.sexe, scy.present, st.matricule, scy.idStudentClassAYear,
                    scy.redouble
            FROM  Student st, ClassAYear cy, StudentClassAYear scy
            WHERE cy.idClassAYear = :id_class_a_year and scy.classAYear = cy.idClassAYear
                and scy.student = st.idStudent
        ) t1
        LEFT JOIN 
        (   
            SELECT st.idStudent, st.studentNames, st.studentFirstNames, st.bithDate, st.matricule,
              st.birthPlace,  scy.idStudentClassAYear, st.sexe, scy.redouble, scy.present,
              mkc.idMarksCategory, mkc.marksCategoryName, ahi.idAbsenceHoursInfo, 
              ahi.abscenceHours, ahi.justifiedAbscenceHours
      
            FROM   AbsenceHoursInfo ahi, StudentClassAYear scy,   Student st , MarksCategory  mkc 
            WHERE  ahi.studentClassAYear = scy.idStudentClassAYear and 
                   scy.student = st.idStudent and scy.classAYear = :id_class_a_year  and 
                   ahi.markCategory = :id_marks_category and mkc.idMarksCategory = ahi.markCategory    
   
        ) t2
        ON t1.idStudentClassAYear = t2.idStudentClassAYear         


