SELECT st.idStudent, st.studentNames, st.studentFirstNames, st.bithDate, 
             st.birthPlace, 
             null as idMarks, null as markCategory, null as mark,
               st.sexe, scy.present, st.matricule, scy.idStudentClassAYear,
             scy.redouble, null as markComment
FROM  Student st, ClassAYear cy, StudentClassAYear scy
WHERE cy.idClassAYear = :id_class_a_year and scy.classAYear = cy.idClassAYear
      and scy.student = st.idStudent" 







SELECT st.idStudent, st.studentNames, st.studentFirstNames, st.bithDate, 
             st.birthPlace, 
             null as idMarks, null as markCategory, null as mark,
               st.sexe, scy.present, st.matricule, scy.idStudentClassAYear,
             scy.redouble, null as markComment
FROM  Student st, ClassAYear cy, StudentClassAYear scy
WHERE cy.idClassAYear = 1 and scy.classAYear = cy.idClassAYear
      and scy.student = st.idStudent
      
      
SELECT st.idStudent, st.studentNames, st.studentFirstNames, st.bithDate, 
     st.birthPlace, mk.idMarks, mk.markCategory, mk.mark, 
     st.sexe, scy.present, st.matricule,  scy.idStudentClassAYear,
     scy.redouble , mk.markComment  
FROM   Marks mk, StudentClassAYear scy,   Student st    
WHERE  mk.studentClassAYear = scy.idStudentClassAYear and 
     scy.student = st.idStudent and scy.classAYear = 1  and 
     mk.markCategory = 1 and mk.subject = 19       






SELECT  t.idSubject, t.subjectName, t.coefficient, t.idClassroomCategory, 
              t.classroomCategoryName, t.idTeachedSubjects as idTeachedSubjects , 
              null  as idCoefficient ,  t2.idTeacher, 
              t2.teacherName, t2.idTeacherClassAYearSubject, t.subjectCategoryId,
              t.subjectCategoryName, t.cycle                         
      FROM  ( SELECT sbj.idSubject, sbj.subjectName, ts.coefficient, ccty.idClassroomCategory, 
                     ccty.classroomCategoryName, ts.idTeachedSubjects,
                     ts.subjectCategoryId, subC.subjectCategoryName, subC.cycle                   
              FROM TeachedSubjects  ts, ClassroomCategory ccty, Subject sbj,
                   SubjectCategory  subC 
              WHERE ts.classroomCategory = :id_classroom_category and           
                    ts.classroomCategory = ccty.idClassroomCategory and 
                    subC.idSubjectCategory = ts.subjectCategoryId and 
                    ts.subject = sbj.idSubject and ts.schoolSchoolYear = :id_schoolschool_year
            ) t 
      LEFT JOIN       
           (
              SELECT tea.idTeacher ,  tea.teacherName ,                             
                     tcys.idTeacherClassAYearSubject, tsub.subject
              FROM   TeacherClassAYearSubject tcys, Teacher tea,
                     TeachedSubjects   tsub
              WHERE tcys.classAYearId = :id_class_a_year and tea.idTeacher = tcys.teacherId 
                    and tsub.idTeachedSubjects = tcys.teachedSubject 
              union
              SELECT null as idTeacher,                  
                      null as teacherName,                             
                      tcys.idTeacherClassAYearSubject, tsub.subject
              FROM  TeacherClassAYearSubject tcys  , TeachedSubjects   tsub 
              WHERE tcys.classAYearId = :id_class_a_year and 
                    tsub.idTeachedSubjects = tcys.teachedSubject  and
                    tcys.teacherId is null 
           ) t2
      ON   t.idSubject = t2.subject 
      order by t.subjectCategoryId, t.idSubject 



SELECT t1.idStudent, t1.studentNames, t1.studentFirstNames, t1.bithDate, 
       t1.birthPlace, t2.idMarks, t2.markCategory, t2.mark,
       t1.sexe, t1.present, t1.matricule, t1.idStudentClassAYear,
       t1.redouble, t2.markComment

FROM
	(
		SELECT st.idStudent, st.studentNames, st.studentFirstNames, st.bithDate, 
			     st.birthPlace, 
                            st.sexe, scy.present, st.matricule, scy.idStudentClassAYear,
			     scy.redouble
		FROM  Student st, ClassAYear cy, StudentClassAYear scy
		WHERE cy.idClassAYear = 1 and scy.classAYear = cy.idClassAYear
		      and scy.student = st.idStudent
	) t1
LEFT JOIN 
	(      
		SELECT st.idStudent, st.studentNames, st.studentFirstNames, st.bithDate, 
		     st.birthPlace, mk.idMarks, mk.markCategory, mk.mark, 
		     st.sexe, scy.present, st.matricule,  scy.idStudentClassAYear,
		     scy.redouble , mk.markComment  
		FROM   Marks mk, StudentClassAYear scy,   Student st    
		WHERE  mk.studentClassAYear = scy.idStudentClassAYear and 
		     scy.student = st.idStudent and scy.classAYear = 1  and 
		     mk.markCategory = 1 and mk.subject = 19     
	) t2
ON t1.idStudentClassAYear = t2.idStudentClassAYear


