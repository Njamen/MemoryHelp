SELECT * 
FROM Student st 
WHERE st.studentNames like '%NDRADA ASSANA%'


SELECT * 
FROM Student st , StudentClassAYear scy
WHERE st.idStudent = scy.student

SELECT * 
FROM Student st , StudentClassAYear scy
WHERE st.idStudent = scy.student and scy.student == 532


SELECT scy.classAYear
FROM Student st , StudentClassAYear scy, ClassAYear cy
WHERE st.idStudent = scy.student and 
      scy.student == 532 and scy.classAYear = cy.idClassAYear
      
      
      SELECT  *
FROM StudentAverage sta
WHERE sta.studentClassAYear = 1533


SELECT * FROM Student st WHERE st.photo_url LIKE ''


