SELECT tea.teacherName , NULL as idLogin,  tpw.teacherId, tpw.password, "" as username, tpw.securityLevel, 1 as active
FROM  Teacher tea ,  TeacherPassword tpw
WHERE  tea.idTeacher = tpw.teacherId




DELIMITER $$ 
CREATE PROCEDURE initialiseLoginTable()  BEGIN
 
 DECLARE v_finished INTEGER  DEFAULT 0;


 DECLARE teacherId  INTEGER;
 DECLARE password varchar(256);
 DECLARE username varchar(256);
 DECLARE securityLevel INTEGER;
 
  

 DEClARE teacher_teacher_password_cursor CURSOR FOR 
 SELECT tpw.teacherId, tpw.password, tea.teacherName  as username, tea.securityLevel
 FROM  Teacher tea ,  TeacherPassword tpw
 WHERE  tea.idTeacher = tpw.teacherId;
  
 DECLARE CONTINUE HANDLER 
        FOR NOT FOUND SET v_finished = 1;   
 
  OPEN teacher_teacher_password_cursor;
   
   get_teacher_teacher_password_Row: LOOP
     
   FETCH teacher_teacher_password_cursor INTO teacherId, password , username, securityLevel ;
  
   IF v_finished = 1 THEN 
    LEAVE get_teacher_teacher_password_Row;
   END IF;
 
           INSERT INTO Login
	   SET idLogin = NULL,
	       teacherId = teacherId,
	       password = password , 	       
	       username = username,
	       securityLevel = securityLevel,
	       active = 1 ;


 
 END LOOP get_teacher_teacher_password_Row;

 CLOSE teacher_teacher_password_cursor;
 
END$$ 
DELIMITER ;





