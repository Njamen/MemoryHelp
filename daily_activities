    private fun bluidDecisionCounselInfo(moy:Float?, absNjust : Int):String {

        var th = if (moy == null ) "" else
                   if (report_card_settings.honor_roll.contains(Mark_value(moy!!)) ) "checked" else ""

        var thE = if (moy == null ) "" else
                   if (report_card_settings.honor_roll_encouragement.contains(Mark_value(moy!!))) "checked" else ""

        var thF = if (moy == null ) "" else
                   if (report_card_settings.honor_roll_congratulations.contains(Mark_value(moy!!))) "checked" else ""
        val bt = if (moy == null ) "" else
                   if (report_card_settings.serious_work_warning.contains(Mark_value(moy!!)))  "checked" else ""
        val at = if (moy == null ) "" else
                    if ((report_card_settings.work_warning.contains(Mark_value(moy!!))) ) "checked" else ""

        val ac = if (report_card_settings.conduct_warning.contains(Mark_value(absNjust.toFloat()))) {
            th = "";
            thE = "";
            thF = "";  "checked"
        } else ""
        val bc = if (report_card_settings.serious_conduct_warning.contains(Mark_value(absNjust.toFloat()))){
            th = "";
            thE = "";
            thF = "";  "checked"
        } else ""
        
