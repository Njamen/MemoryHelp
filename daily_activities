        SELECT  t1.idStudent, t1.studentNames, t1.studentFirstNames, 
                DATE_FORMAT(t1.bithDate, '%Y-%m-%d') as bithDate , 
                t1.birthPlace, t1.idStudentClassAYear,
                t1.average ,  t1.classPosition, t1.sexe , t1.present ,
                t1.matricule , t1.somme , t1.sommeCoef, t1.redouble, 
                t2.idAbsenceHoursInfo, t2.abscenceHours, t2.justifiedAbscenceHours 

        FROM(  SELECT st.idStudent, st.studentNames, st.studentFirstNames, st.bithDate, st.birthPlace, scy.idStudentClassAYear,
                        null as average ,  null as classPosition, st.sexe , scy.present ,
                        st.matricule , null as somme , null as sommeCoef, scy.redouble, 
                        null as idAbsenceHoursInfo, null as abscenceHours, null as justifiedAbscenceHours 
                FROM   StudentClassAYear scy,   Student st 
                WHERE  scy.student = st.idStudent and scy.classAYear = 2 
             ) t1          
        LEFT JOIN (
            SELECT st.idStudent,  scy.idStudentClassAYear,               
                    ahi.idAbsenceHoursInfo, ahi.abscenceHours, ahi.justifiedAbscenceHours 
            FROM   StudentClassAYear scy,   Student st , AbsenceHoursInfo ahi    
            WHERE  scy.student = st.idStudent and scy.classAYear = 2 and
                    scy.idStudentClassAYear = ahi.studentClassAYear 
                    and ahi.markCategory = 1
        ) t2
        ON  t1.idStudentClassAYear  = t2.idStudentClassAYear
        Order by t1.studentNames asc 


/ student average

SELECT t3.idStudent , t3.studentNames , t3.studentFirstNames , 
                t3.bithDate, t3.birthPlace, t3.idStudentClassAYear, t3.average,
                t3.classPosition, t3.sexe, t3.present , t3.matricule,  t4.somme,
                t4.sommeCoef, t3.redouble , t3.idAbsenceHoursInfo, t3.abscenceHours, 
                t3.justifiedAbscenceHours, t4.idStudentAverage, t4.studentClassAYear, t4.average
        FROM (
            SELECT  t1.idStudent , t1.studentNames , t1.studentFirstNames , 
                    t1.bithDate, t1.birthPlace, t1.idStudentClassAYear, t1.average,
                    t1.classPosition, t1.sexe, t1.present , t1.matricule,  t1.somme,
                    t1.sommeCoef, t1.redouble , t2.idAbsenceHoursInfo, t2.abscenceHours, 
                            t2.justifiedAbscenceHours

            FROM(  
                select st.idStudent , st.studentNames , st.studentFirstNames , 
                        st.bithDate, st.birthPlace, scy.idStudentClassAYear, null as  average,
                        null as classPosition, st.sexe, scy.present , st.matricule,  null as somme,
                        null as sommeCoef, scy.redouble
                from  StudentClassAYear scy , Student st, ClassAYear cy
                where cy.idClassAYear = 4 and cy.idClassAYear = scy.classAYear
                            and  st.idStudent = scy.student
                ) t1          
            LEFT JOIN (
                select abi.idAbsenceHoursInfo, abi.abscenceHours, 
                                abi.justifiedAbscenceHours, abi.studentClassAYear
                from AbsenceHoursInfo abi
                where abi.markCategory = 1
            ) t2
            ON  t1.idStudentClassAYear  = t2.studentClassAYear 
        ) t3
        LEFT JOIN (
            SELECT sta.idStudentAverage, sta.studentClassAYear, sta.average,
                   sta.somme, sta.sommeCoef
            FROM StudentAverage sta 
            WHERE sta.markCategory = 1
        ) t4
    ON  t3.idStudentClassAYear  = t4.studentClassAYear
    Order by t3.studentNames asc  


